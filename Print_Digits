#include <iostream>
using namespace std;

void func(int n) {
    if (n == 0) {
        cout << "0 ";
        return;
    }

    if (n / 10 != 0) {
        func(n / 10);
    }
    
    cout << n % 10 << " ";
}

int main() {
    int t;
    cin >> t;
    
    while (t--) {
        int n;
        cin >> n;
    
        func(n);
        cout << endl;
    }

    return 0;
}
