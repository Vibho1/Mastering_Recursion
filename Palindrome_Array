#include <bits/stdc++.h>
using namespace std;

bool func(vector<int>& vec, int left, int right) {
    if(left >= right) return true;
    
    if(vec[left] != vec[right]) return false;
    
    return func(vec, left + 1, right - 1);
}

int main() {
    int n;
    cin >> n;
    vector<int> vec(n);
    
    for(int i = 0; i < n; i++) {
        cin >> vec[i];    
    }
    func(vec, 0, vec.size() - 1) == true ? cout << "YES" << endl : cout << "NO" << endl;
}
