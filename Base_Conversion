#include <bits/stdc++.h>
using namespace std;

void func(int n, string& s) {
    if(n <= 0) return;
    
    int rem = n % 2;
    func(n / 2, s);
    s += to_string(rem);
}

int main() {
    int t;
    cin >> t;
    
    while (t--) {
        int n;
        cin >> n;
        
        string s = "";
        func(n, s);
        cout << s << endl;
    }

    return 0;
}
