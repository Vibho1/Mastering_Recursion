#include <bits/stdc++.h>
using namespace std;

 void helper(int n, vector<int>& vec, int maxi, int i) {
    
    cout << maxi << " ";
    if(i == n) return;
    
    maxi = max(maxi, vec[i]);
    
    helper(n, vec, maxi, i + 1);
}

int main() {

        int n;
        cin >> n;
        
        vector<int> vec(n);
        
        for(int i = 0; i < n; i++) cin >> vec[i];
        
        helper(n, vec, vec[0], 1);
        cout << endl;
        return 0;
}
