#include <bits/stdc++.h>
using namespace std;

int func(vector<int>& vec, int maxi, int i) {
    if (i == vec.size()) return maxi;
    
    maxi = max(maxi, vec[i]);
    
    return func(vec, maxi, i + 1);
}

int main() {
    long long n;
    cin >> n;
    vector<int> vec(n);
    for(int i = 0; i < n; i++) {
        cin >> vec[i];
    }
    cout << func(vec, INT_MIN, 0) << endl;
}
